oTime","parseInt","del","get_obj_after","func","is_view","bl","$check_action","log","uploadimg","uploadFile","file","created"],"sources":["src/views/recharge_order/view.vue"],"sourcesContent":["<template>\n\t<el-main class=\"bg edit_wrap\">\n\t\t<el-form ref=\"form\" :model=\"form\" status-icon label-width=\"120px\" v-if=\"is_view()\">\n\t\t\t<el-col v-if=\"user_group === '管理员' || $check_field('get','user') || $check_field('add','user') || $check_field('set','user')\" :xs=\"24\" :sm=\"12\" :lg=\"8\" class=\"el_form_item_warp\">\n\t\t\t\t<el-form-item label=\"用户\" prop=\"user\">\n\t\t\t\t\t\t<div v-if=\"user_group !== '管理员'\">\n\t\t\t\t\t\t\t{{ get_user_session_user(form['user']) }}\n\t\t\t\t\t\t\t<!--<el-input id=\"business_name\" v-model=\"form['user']\" placeholder=\"请输入用户\"-->\n\t\t\t\t\t\t\t<!--v-if=\"user_group === '管理员' || (form['recharge_order_id'] && $check_field('set','user')) || (!form['recharge_order_id'] && $check_field('add','user'))\" :disabled=\"disabledObj['user_isDisabled']\"></el-input>-->\n\t\t\t\t\t\t\t<!--<div v-else-if=\"$check_field('get','user')\">{{form['user']}}</div>-->\n\t\t\t\t\t\t\t<el-select v-if=\"user_group === '管理员' || (form['recharge_order_id'] && $check_field('set','user')) || (!form['recharge_order_id'] && $check_field('add','user'))\" id=\"user\" v-model=\"form['user']\" :disabled=\"disabledObj['user_isDisabled']\">\n\t\t\t\t\t\t\t\t<el-option v-for=\"o in list_user_user\" :key=\"o['username']\" :label=\"o['nickname'] + '-' + o['username']\"\n\t\t\t\t\t\t\t\t\t\t   :value=\"o['user_id']\">\n\t\t\t\t\t\t\t\t</el-option>\n\t\t\t\t\t\t\t</el-select>\n\t\t\t\t\t\t\t<el-select v-else-if=\"$check_field('get','user')\" id=\"user\" v-model=\"form['user']\" :disabled=\"true\">\n\t\t\t\t\t\t\t\t<el-option v-for=\"o in list_user_user\" :key=\"o['username']\" :label=\"o['nickname'] + '-' + o['username']\"\n\t\t\t\t\t\t\t\t\t\t   :value=\"o['user_id']\">\n\t\t\t\t\t\t\t\t</el-option>\n\t\t\t\t\t\t\t</el-select>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<el-select v-else id=\"user\" v-model=\"form['user']\" :disabled=\"disabledObj['user_isDisabled']\">\n\t\t\t\t\t\t\t<el-option v-for=\"o in list_user_user\" :key=\"o['username']\" :label=\"o['nickname'] + '-' + o['username']\"\n\t\t\t\t\t\t\t\t\t   :value=\"o['user_id']\">\n\t\t\t\t\t\t\t</el-option>\n\t\t\t\t\t\t</el-select>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\t\t\t<el-col v-if=\"user_group === '管理员' || $check_field('get','order_number') || $check_field('add','order_number') || $check_field('set','order_number')\" :xs=\"24\" :sm=\"12\" :lg=\"8\" class=\"el_form_item_warp\">\n\t\t\t\t<el-form-item label=\"订单编号\" prop=\"order_number\">\n\t\t\t\t\t<el-input id=\"order_number\" v-model=\"form['order_number']\" placeholder=\"请输入订单编号\"\n\t\t\t\t\t\t\t  v-if=\"user_group === '管理员' || (form['recharge_order_id'] && $check_field('set','order_number')) || (!form['recharge_order_id'] && $check_field('add','order_number'))\" :disabled=\"true\"></el-input>\n\t\t\t\t\t<div v-else-if=\"$check_field('get','order_number')\">{{form['order_number']}}</div>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\t\t\t<el-col v-if=\"user_group === '管理员' || $check_field('get','recharge_limit') || $check_field('add','recharge_limit') || $check_field('set','recharge_limit')\" :xs=\"24\" :sm=\"12\" :lg=\"8\" class=\"el_form_item_warp\">\n\t\t\t\t<el-form-item label=\"充值额度\" prop=\"recharge_limit\">\n\t\t\t\t\t<el-input-number id=\"recharge_limit\" v-model.number=\"form['recharge_limit']\"\n\t\t\t\t\t\tv-if=\"user_group === '管理员' || (form['recharge_order_id'] && $check_field('set','recharge_limit')) || (!form['recharge_order_id'] && $check_field('add','recharge_limit'))\" :disabled=\"disabledObj['recharge_limit_isDisabled']\"></el-input-number>\n\t\t\t\t\t<div v-else-if=\"$check_field('get','recharge_limit')\">{{form['recharge_limit']}}</div>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\t\t\t<el-col v-if=\"user_group === '管理员' || $check_field('get','recharge_date') || $check_field('add','recharge_date') || $check_field('set','recharge_date')\" :xs=\"24\" :sm=\"12\" :lg=\"8\" class=\"el_form_item_warp\">\n\t\t\t\t<el-form-item label=\"充值日期\" prop=\"recharge_date\">\n\t\t\t\t\t<el-date-picker :disabled=\"disabledObj['recharge_date_isDisabled']\" v-if=\"user_group === '管理员' || (form['recharge_order_id'] && $check_field('set','recharge_date')) || (!form['recharge_order_id'] && $check_field('add','recharge_date'))\" id=\"recharge_date\"\n\t\t\t\t\t\tv-model=\"form['recharge_date']\" type=\"date\" placeholder=\"选择日期\">\n\t\t\t\t\t</el-date-picker>\n\t\t\t\t\t<div v-else-if=\"$check_field('get','recharge_date')\">{{form['recharge_date']}}</div>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\t\t\t<el-col :xs=\"24\" :sm=\"12\" :lg=\"8\" class=\"el_form_btn_warp\">\n\t\t\t\t<el-form-item>\n\t\t\t\t\t<el-button type=\"primary\" @click=\"submit()\">提交</el-button>\n\t\t\t\t\t<el-button @click=\"cancel()\">取消</el-button>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\n\t\t</el-form>\n\t</el-main>\n</template>\n\n<script>\n\timport mixin from \"@/mixins/page.js\";\n\n\texport default {\n\t\tmixins: [mixin],\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tfield: \"recharge_order_id\",\n\t\t\t\turl_add: \"~/api/recharge_order/add?\",\n\t\t\t\turl_set: \"~/api/recharge_order/set?\",\n\t\t\t\turl_get_obj: \"~/api/recharge_order/get_obj?\",\n\t\t\t\turl_upload: \"~/api/recharge_order/upload?\",\n\n\t\t\t\tquery: {\n\t\t\t\t\t\"recharge_order_id\": 0,\n\t\t\t\t},\n\n\t\t\t\tform: {\n\t\t\t\t\t\"user\": 0, // 用户\n\t\t\t\t\t\"order_number\":this.$get_stamp(), // 订单编号\n\t\t\t\t\t\"recharge_limit\":0, // 充值额度\n\t\t\t\t\t\"recharge_date\":'', // 充值日期\n\t\t\t\t\t\"recharge_order_id\": 0, // ID\n\n\t\t\t\t},\n\t\t\t\tdisabledObj:{\n\t\t\t\t\t\"user_isDisabled\": false,\n\t\t\t\t\t\"order_number_isDisabled\": false,\n          \"recharge_limit_isDisabled\": false,\n\t\t\t\t\t\"recharge_date_isDisabled\": false,\n\t\t\t\t},\n\t\t\t\t// 用户列表\n\t\t\t\tlist_user_user: [],\n\t\t\t\t// 用户组\n\t\t\t\tgroup_user_user: \"\",\n\t\t\t}\n\t\t},\n\t\tmethods: {\n\t\t\t/**\n\t\t\t * 获取会员用户用户列表\n\t\t\t */\n\t\t\tasync get_list_user_user() {\n                // if(this.user_group !== \"管理员\" && this.form[\"user\"] === 0) {\n                //     this.form[\"user\"] = this.user.user_id;\n                // }\n                var json = await this.$get(\"~/api/user/get_list?user_group=会员用户\");\n                if(json.result && json.result.list){\n                    this.list_user_user = json.result.list;\n                }\n                else if(json.error){\n                    console.error(json.error);\n                }\n\t\t\t},\n\t\t\t/**\n\t\t\t * 获取会员用户用户组\n\t\t\t */\n\t\t\tasync get_group_user_user() {\n\t\t\t\tthis.form[\"user\"] = this.user.user_id;\n\t\t\t\tvar json = await this.$get(\"~/api/user_group/get_obj?name=会员用户\");\n\t\t\t\tif(json.result && json.result.obj){\n\t\t\t\t\tthis.group_user_user = json.result.obj;\n\t\t\t\t}\n\t\t\t\telse if(json.error){\n\t\t\t\t\tconsole.error(json.error);\n\t\t\t\t}\n\t\t\t},\n\t\t\tget_user_session_user(id){\n\t\t\t\tvar _this = this;\n\t\t\t\tvar user_id = {\"user_id\":id}\n\t\t\t\tvar url = \"~/api/\"+_this.group_user_user.source_table+\"/get_obj?\"\n\t\t\t\tthis.$get(url, user_id, function(res) {\n\t\t\t\t\tif (res.result && res.result.obj) {\n\t\t\t\t\t\tvar arr = []\n\t\t\t\t\t\tfor (let key in res.result.obj) {\n\t\t\t\t\t\t\tarr.push(key)\n\t\t\t\t\t\t}\n\t\t\t\t\t\tvar arrForm = []\n\t\t\t\t\t\tfor (let key in _this.form) {\n\t\t\t\t\t\t\tarrForm.push(key)\n\t\t\t\t\t\t}\n\t\t\t\t\t\tfor (var i=0;i<arr.length;i++){\n\t\t\t\t\t\t  if (arr[i]!=='examine_state' && arr[i]!=='examine_reply') {\n                for (var j = 0; j < arrForm.length; j++) {\n                  if (arr[i] === arrForm[j]) {\n                    if (arr[i] !== \"user\") {\n                      _this.form[arrForm[j]] = res.result.obj[arr[i]]\n                      _this.disabledObj[arrForm[j] + '_isDisabled'] = true\n                      br